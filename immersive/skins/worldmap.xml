<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">

    <Frame name="GwWorldmapHeader" virtual="true">
        <Size x="170" y="64"/>
        <Anchors>
          <Anchor point="BOTTOMLEFT" relativePoint="TOPLEFT" x="0" y="-32" />
          <Anchor point="BOTTOMRIGHT" relativePoint="TOPRIGHT" x="0" y="-32" />
        </Anchors>
        <Layers>
          <Layer level="ARTWORK" textureSubLevel="1">
              <Texture parentKey="windowIcon" file="Interface\AddOns\GW2_UI\textures\character\worldmap-window-icon">
                  <Size x="84" y="84"></Size>
                  <Anchors>
                      <Anchor point="CENTER" relativeKey="$parent.BGLEFT" relativePoint="LEFT" x="21" y="0" />
                  </Anchors>
              </Texture>
            </Layer>
            <Layer level="BACKGROUND" textureSubLevel="2">
                <Texture parentKey="BGLEFT" file="Interface/AddOns/GW2_UI/textures/bag/bagheader">
                    <Size x="512" y="41"></Size>
                    <Anchors>
                        <Anchor point="BOTTOMLEFT" relativePoint="BOTTOMLEFT" x="-2" y="0" />
                    </Anchors>
                    <TexCoords left="0" right="1" top="0.35" bottom="1"/>
                </Texture>
                <Texture parentKey="BGRIGHT" file="Interface/AddOns/GW2_UI/textures/bag/bagheader-right">
                    <Size x="512" y="64"></Size>
                    <Anchors>
                        <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="0" y="0" />
                    </Anchors>

                </Texture>
            </Layer>

        </Layers>
    </Frame>
    <Frame name="GwLightButtonBorder" virtual="true">
        <Size x="170" y="64"/>
        <Anchors>
          <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="0" y="0" />
          <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="0" y="0" />
        </Anchors>
        <Layers>
          <Layer level="ARTWORK" textureSubLevel="1">
              <Texture parentKey="left" file="Interface\AddOns\GW2_UI\textures\uistuff\buttonlight-borderVertical">
                  <Size x="1" y="84"></Size>
                  <Anchors>
                      <Anchor point="TOPLEFT"  relativePoint="TOPLEFT" x="0" y="0" />
                      <Anchor point="BOTTOMLEFT"  relativePoint="BOTTOMLEFT" x="0" y="0" />
                  </Anchors>
              </Texture>
              <Texture parentKey="right" file="Interface\AddOns\GW2_UI\textures\uistuff\buttonlight-borderVertical">
                  <Size x="1" y="84"></Size>
                  <Anchors>
                      <Anchor point="TOPRIGHT"  relativePoint="TOPRIGHT" x="0" y="0" />
                      <Anchor point="BOTTOMRIGHT"  relativePoint="BOTTOMRIGHT" x="0" y="0" />
                  </Anchors>
                  <TexCoords left="1" right="0" top="0" bottom="1" />
              </Texture>
              <Texture parentKey="top" file="Interface\AddOns\GW2_UI\textures\uistuff\buttonlight-borderHorizontal">
                  <Size x="2" y="1"></Size>
                  <Anchors>
                      <Anchor point="TOPLEFT"  relativePoint="TOPLEFT" x="0" y="0" />
                      <Anchor point="TOPRIGHT"  relativePoint="TOPRIGHT" x="0" y="0" />
                  </Anchors>
              </Texture>
            </Layer>
        </Layers>
    </Frame>

</Ui>
