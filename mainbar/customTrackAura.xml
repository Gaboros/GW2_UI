
<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">

    <Button name="GwCustomAuraTrackerTemplate" virtual="true">
        <Size x="50" y="50"/>
        <Layers>
            <Layer level="ARTWORK">
                <FontString parentKey="counter" inherits="NumberFontNormal" justifyH="LEFT" text="11">
                    <Size x="15" y="15"/>
                    <Anchors>
                        <Anchor point="BOTTOMRIGHT" x="2" y="2"/>
                    </Anchors>
                </FontString>
            </Layer>
            <Layer level="ARTWORK">
                <Texture name="$parentIcon" parentKey="icon">
                    <Size x="50" y="50"></Size>
                    <Anchors>
                        <Anchor point="CENTER"></Anchor>
                    </Anchors>
                </Texture>
            </Layer>
        </Layers>
        <Frames>
            <Cooldown parentKey="cooldown" inherits="CooldownFrameTemplate" hidden="false"/>
        </Frames>
    </Button>

</Ui>