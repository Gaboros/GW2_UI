<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">

    <Frame name="GwPlayerPowerBar" virtual="true">
        <Size x="318" y="20"></Size>
        <Anchors>
            <Anchor point="BOTTOMLEFT" relativePoint="BOTTOM" x="53" y="86"></Anchor>
        </Anchors>
        <Layers>
            <Layer level="BACKGROUND">
              <Texture name="$parentBackground" file="Interface/AddOns/GW2_UI/textures/uistuff/statusbar">
                  <Size x="318" y="20"></Size>
              </Texture>
                <Texture file="Interface/AddOns/GW2_UI/textures/uistuff/statusbarBorderPixel">
                    <Size x="314" y="2"/>
                    <Anchors>
                        <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="0" y="1"/>
                        <Anchor point="TOPRIGHT" relativePoint="TOPRIGHT"  x="0" y="1"/>
                    </Anchors>
                </Texture>
                <Texture file="Interface/AddOns/GW2_UI/textures/uistuff/statusbarBorderPixel">
                    <Size x="314" y="2"/>
                    <Anchors>
                        <Anchor point="BOTTOMLEFT" relativePoint="BOTTOMLEFT" x="0" y="-1"/>
                        <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT"  x="0" y="-1"/>
                    </Anchors>
                    <TexCoords left="0" right="1" top="1" bottom="0" />
                </Texture>

            <Texture file="Interface/AddOns/GW2_UI/textures/uistuff/statusbarBorderPixelVertical">
                <Size x="2" y="2"/>
                <Anchors>
                    <Anchor point="TOPRIGHT" relativePoint="TOPRIGHT" x="1" y="0"/>
                    <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="1" y="0"/>
                </Anchors>
            </Texture>
            <Texture file="Interface/AddOns/GW2_UI/textures/uistuff/statusbarBorderPixelVertical">
                <Size x="2" y="2"/>
                <Anchors>
                    <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="-1" y="0"/>
                    <Anchor point="BOTTOMLEFT" relativePoint="BOTTOMLEFT" x="-1" y="0"/>
                </Anchors>
                <TexCoords left="1" right="0" top="0" bottom="1" />
            </Texture>
            </Layer>
            <Layer level="BORDER">
                <Texture parentKey="bar" file="Interface/AddOns/GW2_UI/textures/uistuff/gwstatusbar">
                    <Size x="314" y="18"/>
                    <Anchors>
                        <Anchor point="LEFT" x="1" y="0"/>
                        <Anchor point="RIGHT" relativePoint="LEFT" x="0" y="0"/>
                    </Anchors>
                </Texture>
            </Layer>
        </Layers>
        <Frames>
            <StatusBar parentKey="candy" name="$parentCandy" frameLevel="1" defaultValue="1" minValue="0" maxValue="1" drawlayer="BACKGROUND">
                <Size x="314" y="18"/>
                <Anchors>
                    <Anchor point="LEFT" x="2" y="0"/>
                </Anchors>
                <BarTexture file="Interface/AddOns/GW2_UI/textures/uistuff/gwstatusbarCandy"/>
                <Layers>
                    <Layer>
                        <Texture parentKey="spark" name="$parentSpark" file="Interface/AddOns/GW2_UI/textures/hud/bloodspark">
                            <Anchors>
                                <Anchor point="RIGHT" x="0" y="0"/>
                            </Anchors>
                            <Size x="21" y="18"></Size>
                        </Texture>
                    </Layer>
                </Layers>
            </StatusBar>
            <StatusBar parentKey="statusBar" name="$parentBar" defaultValue="1" minValue="0" maxValue="1">
                <Size x="314" y="18"/>
                <Anchors>
                    <Anchor point="LEFT" x="2" y="0"/>
                </Anchors>
                <BarTexture file="Interface/AddOns/GW2_UI/textures/uistuff/gwstatusbar"/>
                <Layers>
                    <Layer level="OVERLAY">
                        <FontString parentKey="label" name="$parentString" text="unknown" inherits="GameFontNormalSmall" justifyH="CENTER">
                            <Color r="1" b="1" g="1"></Color>
                            <Size x="316" y="18"/>
                            <Anchors>
                                <Anchor point="CENTER" x="0" y="0"/>
                            </Anchors>
                        </FontString>

                    </Layer>
                </Layers>
            </StatusBar>
        </Frames>
    </Frame>

</Ui>
